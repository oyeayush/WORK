#include <stdio.h>
#include <stdlib.h>
void swap(int*,int*);
int main()
{
    int a=10,b=20;
    printf("\n Befoe swapping the values are %d and %d",a,b);
    swap(&a,&b);
    printf("\n After swapping values are %d and %d",a,b);
}
 void swap(int* x,int* y)
{
    int temp;
    temp = *x;
    *x = *y;
    *y = temp;
    printf("\nafter swapping values are %d and %d",*x,*y);

}


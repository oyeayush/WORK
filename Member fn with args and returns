#include <iostream>
#include<conio.h>
#include<string.h>

using namespace std;


class test
{
   int a,b;
   public:
   void readdata(int x, int y)

   {
       a=x;
       b=y;
   }

   void showdata()

   {
       cout<<"a is:"<<a<<endl;
       cout<<"b is:"<<b<<endl;

   }


   int big()
   {
       if(a>b)

           return a;

       else

           return b;

   }
};

  int main()
{
    test t;
    t.readdata(5,7);
    t.showdata();
    int max = t.big();
    cout<<"Big number is:"<<max<<endl;
}


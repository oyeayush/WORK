#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n;
    printf("enter a number::");
    scanf("%d",&n);
    switch(n)
    {
        case 1 : printf("ONE");
        break;
        case 2 : printf("TWO");
        break;
        case 3 : printf("THREE");
        break;
        case 4 : printf("FOUR");
        break;
        default : printf("INVALID INPUT");

    }
    return 0;
}

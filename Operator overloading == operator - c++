#include <iostream>
#include<conio.h>
#include<string.h>

using namespace std;


class test
{
    int a;
public:
    void get()
    {
       cin>>a;
    }
         void compare(test t2)                       //explicit obj
         {
            if(a==t2.a)
                cout<<"objects are equal"<<endl;
            else
                cout<<"objects are not equal"<<endl;
         }
};


int main()
{
    test t1,t2;
    cout<<"enter t1 object a value:"<<endl;
    t1.get();
cout<<"enter t2 object a value:"<<endl;
    t2.get();
   t1.compare(t2);
}

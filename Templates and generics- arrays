#include <iostream>
#include<conio.h>
#include<string.h>

using namespace std;

template <class t>
t sum(t arr[], int size)
{
    t s=0;
    for(int i=0;i<size;i++)
    {
        s=s+arr[i];
    }
        return s;

}



int main()
{   int x[5]={5,6,7,8,9};
float y[3]={1.1,2.2,3.3};
    cout<<"integer sum ="<<sum(x, 5)<<endl;                   //calling
    cout<<"float sum ="<<sum(y, 3)<<endl;                   //calling

}

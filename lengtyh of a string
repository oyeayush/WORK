#include <stdio.h>
#include <stdlib.h>

int stringlength(char[]);
void main()
{
    char str[50];
    int len;
    printf("\nenter string:");
    gets(str);
    len = stringlength(str);
    printf("\nlength of the string is : %d",len);
}
int stringlength(char str[])
{
    int i=0;
    int count=0;
    while(str[i] != '\0')
        count++;
        i++;
return count;
}


